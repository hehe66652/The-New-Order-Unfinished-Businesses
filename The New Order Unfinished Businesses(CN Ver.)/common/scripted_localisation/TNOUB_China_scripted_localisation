

####    Hiding Our Interests GUI    ####

defined_text = {
    name = GetHidingOurInterestsGUIPointName
    text = {
        trigger = {
            has_country_flag = CHI_point_jiangsu
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_name_jiangsu_localization_key
    }
    text = {
        trigger = {
            has_country_flag = CHI_point_jiangxi
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_name_jiangxi_localization_key
    }
    text = {
        trigger = {
            has_country_flag = CHI_point_changde
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_name_changde_localization_key
    }
    text = {
        trigger = {
            has_country_flag = CHI_point_shijiazhuang
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_name_shijiazhuang_localization_key
    }
    text = {
        trigger = {
            has_country_flag = CHI_point_wuhan
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_name_wuhan_localization_key
    }
    text = {
        trigger = {
            has_country_flag = CHI_point_chongqing
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_name_chongqing_localization_key
    }
    text = {
        trigger = {
            has_country_flag = CHI_point_yunnan
        }
