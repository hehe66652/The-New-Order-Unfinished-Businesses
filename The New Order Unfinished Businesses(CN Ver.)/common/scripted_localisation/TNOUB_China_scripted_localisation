            set_temp_variable = { temp1 = CHI_Hiding_Our_Interests_GUI_point_max_yunnan }
            set_temp_variable = { temp2 = CHI_Hiding_Our_Interests_GUI_point_available_yunnan }
            divide_temp_variable = { temp2 = temp1 }
            
            OR = {
                check_variable = { temp2 = 0.25 }
                check_variable = { temp2 < 0.25 }
            }
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_available_low_yunnan_localization_key
    }
}
